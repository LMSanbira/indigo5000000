{
    "name": "Indigo 5,000,000",
    "version": "1.02",
    "description": "Brings the indigo to you!",
    "manifest_version": 2,
    "icons": { 
        "16": "indigo_icon.png",
        "48": "indigo_icon.png",
        "128": "128indigo.png" 
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Indigo 5,000,000"
    },
    "permissions": [
        "tabs"
    ]
}